<FindBugsFilter>
    <!-- Fallback excludes file.  This file will be used when a project lacks a findbugs-excludes.xml file -->
    <Match>
        <Source name="~.*\.groovy" />
        <Or>
            <Field name="~__.*" />
            <Field name="~\$.*" />
            <Method name="~\$.*" />
        </Or>
    </Match>
    <Match>
        <Source name="~.*\.groovy" />
        <Bug code="UPM" />
    </Match>
</FindBugsFilter>